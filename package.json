    "path-to-regexp": "6.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flexbox-grid": "2.1.2",
    "react-hot-loader": "4.13.0",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
